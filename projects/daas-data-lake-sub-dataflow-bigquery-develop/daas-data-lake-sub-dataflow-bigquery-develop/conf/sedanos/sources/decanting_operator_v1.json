{
  "schema_name": "decanting_operator",
  "schema_description": "Contains persons that could decant products.",
  "schema_version": "v1",
  "labels": {},
  "fields": [
    {
      "description": "Client name.",
      "mode": "REQUIRED",
      "name": "__client_name",
      "type": "STRING"
    },
    {
      "description": "Source name.",
      "mode": "REQUIRED",
      "name": "__source_name",
      "type": "STRING"
    },
    {
      "description": "Schema version.",
      "mode": "REQUIRED",
      "name": "__schema_version",
      "type": "STRING"
    },
    {
      "description": "Event timestamp.",
      "mode": "REQUIRED",
      "name": "__event_timestamp",
      "type": "TIMESTAMP"
    },
    {
      "name": "id",
      "type": "INTEGER",
      "mode": "REQUIRED",
      "description": "A unique auto-generated number."
    },
    {
      "name": "user_id",
      "type": "STRING",
      "mode": "REQUIRED",
      "description": "A system-wide unique identifier for the user."
    },
    {
      "name": "username",
      "type": "STRING",
      "mode": "REQUIRED",
